        "erusev/parsedown": "^1.6.4",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "ext-iconv": "*",
        "mobiledetect/mobiledetectlib": "^2.8.39",
        "patchwork/utf8": "^1.3.1",
        "php": "^8.1",
        "phpoffice/phpspreadsheet": "^1.23.0",
        "psr/simple-cache": "^1.0.0",
        "soundasleep/html2text": "^0.5.0",
        "symfony/yaml": "^4.0",
        "tecnickcom/tcpdf": "^6.2.16",
        "twig/twig": "^2.4.4",
        "laminas/laminas-authentication": "~2.10.1",
