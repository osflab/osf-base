        "laminas/laminas-hydrator": "~4.3",
        "laminas/laminas-i18n": "~2.15",
        "laminas/laminas-i18n-resources": "~2.8",
        "laminas/laminas-paginator": "~2.12.2",
        "laminas/laminas-permissions-acl": "~2.9.0",
        "laminas/laminas-validator": "~2.19.0",
        "laminas/laminas-view": "~2.20.0"
    },
    "require-dev": {
        "laminas/laminas-code": "~4.5.2"
    },
    "replace": {
        "osflab/application": "self.version",
        "osflab/bean": "self.version",
        "osflab/cache": "self.version",
        "osflab/config": "self.version",
        "osflab/console": "self.version",
        "osflab/container": "self.version",
        "osflab/controller": "self.version",
        "osflab/crypt": "self.version",
        "osflab/db": "self.version",
        "osflab/device": "self.version",
        "osflab/doc-maker": "self.version",
        "osflab/exception": "self.version",
        "osflab/filter": "self.version",
        "osflab/form": "self.version",
        "osflab/generator": "self.version",
        "osflab/github": "self.version",
        "osflab/helper": "self.version",
        "osflab/image": "self.version",
        "osflab/log": "self.version",
        "osflab/navigation": "self.version",
        "osflab/office": "self.version",
        "osflab/pdf": "self.version",
        "osflab/safety": "self.version",
        "osflab/session": "self.version",
        "osflab/stream": "self.version",
        "osflab/test": "self.version",
        "osflab/validator": "self.version",
        "osflab/view": "self.version"
    },
    "suggest": {
        "ext-yaml": "For better performances",
        "zendframework/ZendPdf": "If you want to use this deprecated feature",
        "zendframework/zend-authentication": "~2.5.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Osf\\": "src"
        }
    }
